{
  "name": "80-full-stack-nats",
  "version": "1.0.0",
  "private": true,
  "description": "A full-stack microservices example using SyntropyLog with NATS.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "concurrently \"npm:start:api-gateway\" \"npm:start:sales-service\" \"npm:start:dispatch-service\"",
    "start:api-gateway": "node dist/api-gateway/index.js",
    "start:sales-service": "node dist/sales-service/index.js",
    "start:dispatch-service": "node dist/dispatch-service/index.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "nats": "^2.17.0",
    "syntropylog": "^0.6.4-alpha.0",
    "zod": "^3.22.4",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.2",
    "concurrently": "^8.2.1",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
}
