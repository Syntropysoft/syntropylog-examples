{
  "name": "28-testing-patterns",
  "version": "1.0.0",
  "description": "Testing patterns for SyntropyLog - showing how to test declaratively",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "vitest --run --reporter=verbose",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:simple": "vitest --config vitest.config.simple.ts"
  },
  "dependencies": {
    "syntropylog": "0.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0"
  }
}
